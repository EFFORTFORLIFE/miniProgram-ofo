.checkboxes{
  padding:20rpx 20rpx;
  display:flex;
  flex-wrap:wrap;
  box-sizing: border-box;
  justify-content: space-between;
}
.checkboxItem{
  width:48%;
  box-sizing:border-box;
  margin:10rpx 0;
  border:1px dashed #b4aeb4;
  padding:10rpx 10rpx;
  border-radius: 5rpx;
}
.images{
  display: flex;
  flex-wrap: wrap;
}
.images image,.images view{
  width:160rpx;
  height: 160rpx;
  border:1px dashed#b4aeb4;
  margin:15rpx;
  padding:15rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.images view{
  text-align: center;
  vertical-align: middle;
  color:#b4aeb4;
  font-size: 60rpx;
}
.detail input{
  background:#effff1;
  margin:10rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 10rpx;
  border:1px dashed #b4aeb4;
  padding-left:10rpx;
  text-overflow: ellipsis;
}
.submitBtn{
  margin:10rpx;
}